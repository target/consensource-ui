    "outDir": "./dist/",
    "sourceMap": true,
    "allowJs": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "sourceMap": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "App": [
        "./src/*"
      ]
    },
  },
  "include": [
    "./src/**/*.ts"
  ]
}
