{
    "name": "consensource-ui",
    "version": "0.1.0",
    "description": "ConsenSource UI",
    "main": "dist/retailer.js",
    "scripts": {
        "clean": "rm -f src/protobuf/index.* public/js/*.js",
        "generate-protobuf": "./scripts/compile_protobufs.sh",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "npm run generate-protobuf && mkdir -p ./public/js && webpack",
        "watch": "npm run generate-protobuf && mkdir -p ./public/js && webpack --watch --progress --mode=development --env.ENVIRONMENT=development",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --fix",
        "format": "eslint --fix src"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/target/consensource-ui"
    },
    "keywords": [
        "webclient"
    ],
    "author": "",
    "license": "UNLICENSED",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.23.0",
        "@typescript-eslint/parser": "^2.23.0",
        "dotenv": "^8.2.0",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "prebuild-install": "^5.0.0",
        "prettier": "^1.19.1",
        "protobufjs": "^6.8.8",
        "source-map-loader": "^0.2.4",
        "terser-webpack-plugin": "1.4.2",
        "ts-loader": "^6.2.1",
        "typescript": "^3.8.3",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0"
    },
    "dependencies": {
        "eventsource": "^1.0.7",
        "mithril": "^1.1.6",
        "mithril-datepicker": "^0.9.2",
        "sawtooth-sdk": "^1.0.5",
        "sjcl": "^1.0.7",
        "uuid": "^3.3.2"
    },
    "loglevel": "error"
}
