{
  "name": "consensource-ui",
  "version": "0.1.0",
  "description": "ConsenSource UI",
  "main": "dist/retailer.js",
  "scripts": {
    "clean": "rm -f src/compiled_protobufs.json public/js/*.js",
    "generate-protobuf": "./scripts/compile_protobufs.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run generate-protobuf && mkdir -p ./public/js && webpack",
    "watch": "npm run generate-protobuf && mkdir -p ./public/js && webpack --watch --progress --mode=development --env.ENVIRONMENT=development",
    "lint": "eslint src",
    "format": "eslint --fix src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/target/consensource-ui"
  },
  "keywords": [
    "webclient"
  ],
  "author": "",
  "license": "UNLICENSED",
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-plugin-es-beautifier": "^1.0.1",
    "prebuild-install": "^5.0.0",
    "terser-webpack-plugin": "1.4.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "eventsource": "^1.0.7",
    "mithril": "^1.1.6",
    "mithril-datepicker": "^0.9.2",
    "protobufjs": "^6.8.8",
    "sawtooth-sdk": "^1.0.5",
    "sjcl": "^1.0.7",
    "typescript": "^3.8.3",
    "uuid": "^3.3.2"
  },
  "loglevel": "error"
}
