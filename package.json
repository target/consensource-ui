{
    "name": "consensource-ui",
    "version": "0.1.0",
    "description": "ConsenSource UI",
    "main": "dist/retailer.js",
    "scripts": {
        "clean": "rm -f public/js/*.js",
        "generate-protobufs": "./scripts/compile_protobufs.sh",
        "test": "jest",
        "test:coverage": "jest --collect-coverage",
        "build": "npm run generate-protobufs && mkdir -p ./public/js && webpack",
        "watch": "npm run generate-protobufs && mkdir -p ./public/js && webpack --watch --progress --mode=development --env.ENVIRONMENT=development",
        "lint": "npm run generate-protobufs && eslint 'src/**/*.{js,ts,tsx}' --fix --ignore-pattern 'src/protobuf/*'",
        "format": "eslint --fix --ignore-pattern 'src/protobuf/*' src",
        "start:dev": "webpack-dev-server --open --hot"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/target/consensource-ui"
    },
    "keywords": [
        "webclient"
    ],
    "author": "",
    "license": "UNLICENSED",
    "devDependencies": {
        "@types/eventsource": "^1.1.2",
        "@types/jest": "^25.1.4",
        "@types/mithril": "^2.0.2",
        "@types/sawtooth-sdk": "^1.1.0",
        "@types/sjcl": "^1.0.29",
        "@types/uuid": "^7.0.0",
        "@types/webpack": "^4.41.8",
        "@typescript-eslint/eslint-plugin": "^2.23.0",
        "@typescript-eslint/parser": "^2.23.0",
        "dotenv": "^8.2.0",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^25.1.0",
        "jest-localstorage-mock": "^2.4.0",
        "prebuild-install": "^5.0.0",
        "prettier": "^1.19.1",
        "protobufjs": "^6.8.8",
        "source-map-loader": "^0.2.4",
        "terser-webpack-plugin": "1.4.2",
        "ts-jest": "^25.2.1",
        "ts-loader": "^6.2.1",
        "typescript": "^3.8.3",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.3.0",
        "webpack-dev-server": "^3.10.3"
    },
    "dependencies": {
        "eventsource": "^1.0.7",
        "mithril": "^2.0.2",
        "mithril-datepicker": "^0.9.2",
        "sawtooth-sdk": "^1.0.5",
        "sjcl": "^1.0.7",
        "uuid": "^3.3.2"
    },
    "loglevel": "error"
}
